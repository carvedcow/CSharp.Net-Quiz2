<Window x:Class="CSharp.Net_Quiz2.Question4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharp.Net_Quiz2"
        mc:Ignorable="d"
        Title="Question4" Height="250" Width="300">
    <DockPanel Margin="10">
        <StackPanel  Margin="0,0,10,0">
            <TextBox x:Name="tbStudentID"></TextBox>
            <Button x:Name="btnShowStudent" Click="btnShowStudent_Click">Add User</Button>
        </StackPanel>
        <ListBox DockPanel.Dock="Right" Name="lbStudents" DisplayMemberPath="Name"></ListBox>
    </DockPanel>
</Window>
